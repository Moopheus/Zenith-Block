{
  "name": "Zenith Block",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Chrome URL Blocking Extension",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "blocked.png"
  }
}
